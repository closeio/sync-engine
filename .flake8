[flake8]
ignore=
    # TODO: undefined name 'name'
    F821
    # TODO: 'module' imported but unused
    F401
    # TODO: expected 2 blank lines after class or function definition
    E305
    # TODO: too many leading '#' for block comment
    E266
    # do not use bare 'except'
    E722
    # Module level import not at top of file
    E402,
    # This is not PEP8-compliant and conflicts with black.
    E501,
    E231,
    E203,
    W503,
    W504,
    # Use 'class Klass:' instead of 'class Klass(object), can't do this on Python 2
    SIM120,
    # use yield from, can't do this on Python 2
    SIM104,
    # use contextlib.suppress, can't do this on Python 2
    SIM105,
    SIM106,
    SIM110,
    SIM111
exclude=migrations/versions